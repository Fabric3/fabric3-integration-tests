<composite xmlns="http://docs.oasis-open.org/ns/opencsa/sca/200903"
           xmlns:sca="http://docs.oasis-open.org/ns/opencsa/sca/200903"
           xmlns:f3-core="urn:fabric3.org:core"
           xmlns:f3-impl="urn:fabric3.org:implementation"
           xmlns:mct="urn:addnumbers:mct"
           xmlns:mect="urn:helloworld:mect"
           xmlns:sect="urn:helloworld:sect"
           xmlns:seit="urn:helloworld:seit"
           xmlns:f3-binding="urn:fabric3.org:binding"
           targetNamespace="org.codehaus.fabric3"
           name="Java2WsdlTestComposite">

    <component name="MultiComplexTypeTestClient">
        <f3-impl:junit class="org.fabric3.tests.wsdl.matching.multicomplextype.MultiComplexTypeTestClient"/>
        <reference name="portType" target="AddNumbersMultiComplextTypeStub"/>
        <reference name="mappedPortType" target="AddNumbersMultiComplextTypeStub"/>
    </component>

    <component name="MultiElementComplexTypeTestClient">
        <f3-impl:junit class="org.fabric3.tests.wsdl.matching.multielementcomplextype.MultiElementComplexTypeTestClient"/>
        <reference name="portType" target="HelloWorldMultiElementComplexTypeStub"/>
    </component>

    <component name="SingleElementComplexTypeTestClient">
        <f3-impl:junit class="org.fabric3.tests.wsdl.matching.singleelementcomplextype.SingleElementComplexTypeTestClient"/>
        <reference name="portType" target="HelloWorldsingleElementComplexTypeStub"/>
    </component>

    <component name="SingleElementInlineTypeTestClient">
        <f3-impl:junit class="org.fabric3.tests.wsdl.matching.singleelementinlinetype.SingleElementInlineTypeTestClient"/>
        <reference name="portType" target="HelloWorldSingleElementInlineTypeStub"/>
    </component>

    <component name="AddNumbersMultiComplextTypeStub">
        <f3-impl:implementation.wsdl stub="org.fabric3.tests.wsdl.matching.multicomplextype.AddNumbersMultiComplextTypeStub">
            <service contract="mct:AddNumbersPortType"/>
        </f3-impl:implementation.wsdl>
    </component>

    <component name="HelloWorldMultiElementComplexTypeStub">
        <f3-impl:implementation.wsdl stub="org.fabric3.tests.wsdl.matching.multielementcomplextype.HelloWorldMultiElementComplexTypeStub">
            <service contract="mect:HelloWorldPortType"/>
        </f3-impl:implementation.wsdl>
    </component>

    <component name="HelloWorldsingleElementComplexTypeStub">
        <f3-impl:implementation.wsdl stub="org.fabric3.tests.wsdl.matching.singleelementcomplextype.HelloWorldsingleElementComplexTypeStub">
            <service contract="sect:HelloWorldPortType"/>
        </f3-impl:implementation.wsdl>
    </component>

    <component name="HelloWorldSingleElementInlineTypeStub">
        <f3-impl:implementation.wsdl stub="org.fabric3.tests.wsdl.matching.singleelementinlinetype.HelloWorldSingleElementInlineTypeStub">
            <service contract="seit:HelloWorldPortType"/>
        </f3-impl:implementation.wsdl>
    </component>

</composite>
