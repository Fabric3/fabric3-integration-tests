        <property name="dateValue">2007-10-31T01:00:00Z</property>
        <property name="calendarValue">2007-10-31T01:00:00Z</property>
        <property name="mapValue">
            <value>
                <one>1</one>
                <two>2</two>
            </value>
        </property>
        <property name="propertiesValue">
            <value>
                <prop1>value1</prop1>
                <prop2>value2</prop2>
            </value>
        </property>
        <property name="listValue">value1 value2</property>
        <property name="mapOfQNameToClassValue" xmlns:foo="urn:foo">
            <value>
                <foo:one>java.lang.String</foo:one>
                <foo:two>java.util.Date</foo:two>
            </value>
        </property>
        <property name="foo">
            <foo xmlns="">
                <bar>Bar</bar>
            </foo>
        </property>
    </component>

