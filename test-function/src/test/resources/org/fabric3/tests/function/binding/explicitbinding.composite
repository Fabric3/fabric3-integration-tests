<composite xmlns="http://docs.oasis-open.org/ns/opencsa/sca/200903"
           xmlns:f3-core="urn:fabric3.org:core"
           xmlns:f3-impl="urn:fabric3.org:implementation"
           xmlns:f3-binding="urn:fabric3.org:binding"
           name="ExplicitBindingComposite">

    <component name="ExplicitBindingTestComponent">
        <f3-impl:junit class="org.fabric3.tests.function.binding.BoundServiceReferenceTest"/>
        <reference name="helloService">
            <f3-binding:binding.test uri="HelloService"/>
        </reference>
        <reference name="listOfReferences">
            <f3-binding:binding.test name="one" uri="HelloService"/>
            <f3-binding:binding.test name="two" uri="HelloService"/>
        </reference>
    </component>

    <component name="HelloExplicitComponent">
        <implementation.java class="org.fabric3.tests.function.common.HelloServiceImpl"/>
        <service name="HelloService">
            <f3-binding:binding.test uri="HelloService"/>
        </service>
    </component>

</composite>
