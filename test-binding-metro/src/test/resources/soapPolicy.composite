    </component>

    <component name="SOAP11Service">
        <implementation.java class="org.fabric3.tests.binding.metro.soap.Soap11ServiceImpl"/>
        <service name="Soap11Service">
            <binding.ws uri="/soap11Service"/>
        </service>
    </component>


</composite>