<composite xmlns="http://docs.oasis-open.org/ns/opencsa/sca/200912"
           xmlns:sca="http://docs.oasis-open.org/ns/opencsa/sca/200912"
           xmlns:f3-core="urn:fabric3.org:core"
           xmlns:f3-impl="urn:fabric3.org:implementation"
           targetNamespace="org.codehaus.fabric3"
           name="NetJAXBComposite">


    <component name="JAXBWeatherTest">
        <f3-impl:junit class="org.fabric3.tests.binding.net.jaxb.JAXBWeatherTest"/>
        <reference name="weatherService">
            <binding.http uri="http://127.0.0.1:8282/JAXBWeatherService"/>
