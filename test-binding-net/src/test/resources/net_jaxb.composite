    </component>

    <component name="JAXBWeatherService">
        <implementation.java class="org.fabric3.tests.binding.net.jaxb.WeatherServiceImpl"/>
        <service name="WeatherService">
            <binding.http uri="/JAXBWeatherService"/>
        </service>
    </component>


</composite>
