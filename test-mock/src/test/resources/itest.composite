<composite xmlns="http://docs.oasis-open.org/ns/opencsa/sca/200912"
           xmlns:f3-core="urn:fabric3.org:core"
           xmlns:f3-impl="urn:fabric3.org:implementation"
           targetNamespace="org.codehaus.fabric3"
           name="TestComposite"
           autowire="true">

    <component name="TestComponent">
        <f3-impl:junit class="org.fabric3.tests.mock.MockTest"/>
    </component>

    <component name="UserComponent">
        <implementation.java class="org.fabric3.tests.mock.UserComponentImpl"/>
    </component>

    <component name="MockComponent">
        <f3-impl:implementation.mock>
            org.fabric3.tests.mock.MockService1
            org.fabric3.tests.mock.MockService2
            org.fabric3.tests.mock.OverloadedService
        </f3-impl:implementation.mock>
    </component>

</composite>